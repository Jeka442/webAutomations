{
  "manifest_version": 3,
  "name": "webAutomation",
  "version": "1.0",
  "description": "web automation and tools",
  "permissions": ["activeTab", "scripting", "downloads", "contextMenus"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://jsonviewer.stack.hu/*"],
      "js": ["scripts/jsonviewer.js"]
    }
  ]
}
